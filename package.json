    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "all-contributors-cli": "^6.24.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.14.47",
    "obsidian": "latest",
    "prettier": "^2.7.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
